{
  "name": "tinacms-plugin-workspace",
  "version": "0.0.0",
  "description": "Plugin workspace for TinaCMS",
  "author": "mmintel",
  "private": true,
  "license": "MIT",
  "website": "https://mintel.me",
  "repository": "mmintel/tinacms-condition-field",
  "scripts": {
    "test": "wsrun -x example test",
    "test:watch": "wsrun -x example test:watch",
    "build": "wsrun -x example build",
    "predeploy": "wsrun -p example build",
    "deploy": "gh-pages -d example/build",
    "develop": "yarn build && wsrun -p example -r develop",
    "dev": "npm run develop"
  },
  "devDependencies": {
    "wsrun": "^5.2.0",
    "babel-core": "^6.26.3",
    "babel-eslint": "^8.2.5",
    "babel-plugin-external-helpers": "^6.22.0",
    "babel-preset-env": "^1.7.0",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-0": "^6.24.1",
    "cross-env": "^5.1.4",
    "eslint": "^5.0.1",
    "eslint-config-standard": "^11.0.0",
    "eslint-config-standard-react": "^6.0.0",
    "eslint-plugin-import": "^2.13.0",
    "eslint-plugin-node": "^7.0.1",
    "eslint-plugin-promise": "^4.0.0",
    "eslint-plugin-react": "^7.10.0",
    "eslint-plugin-standard": "^3.1.0",
    "gh-pages": "^1.2.0",
    "react": "^16.4.1",
    "react-dom": "^16.4.1",
    "react-scripts": "^1.1.4",
    "rollup": "^0.64.1",
    "rollup-plugin-babel": "^3.0.7",
    "rollup-plugin-commonjs": "^9.1.3",
    "rollup-plugin-node-resolve": "^3.3.0",
    "rollup-plugin-peer-deps-external": "^2.2.0"
  },
  "workspaces": [
    "packages/*",
    "example"
  ]
}
